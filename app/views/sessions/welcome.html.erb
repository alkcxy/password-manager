<h1>Welcome</h1>

<% if logged_in? %>
    <h1>You are Logged In, <%= current_user.name %></h1>

    <%= button_to "Create user", '/users/new', method: :get%>
    <%= button_to "Credentials list", '/credentials', method: :get%>
<% else %>
    <%= button_to "Login", '/login', method: :get%>
<%end%>


